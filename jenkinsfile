pipeline {
   environment {
     git_url = "https://github.com/RubhanRV/website.git"
     git_branch = "master"
   }

  agent {label 'node'}
  //agent any
  stages {
    stage('Pull Source') {
      steps {
        git credentialsId: '4ca32847-933a-4f8d-a160-245be7f4d90e', branch: "${git_branch}", url: "${git_url}"
       
      }
     }
    
    stage('publish web') {
      steps { 
          sh " sudo cp index.html /var/www/html "
          sh " sudo cp -r images   /var/www/html "

      steps { 
      echo "Success"
         
     }
    }
  } 
}
}
